	          	<% else %>
	          	 <li>
	          	<% end %>
	          		<%= link_to "4", facebook_path( :times => 3) %>
	          	</li>
	          	<% if params[:times] == "4" %>
	          	 <li class="active">
	          	<% else %>
	          	 <li>
	          	<% end %>
	          		<%= link_to "5", facebook_path( :times => 4) %>
	          	</li>
	          </ul>	


