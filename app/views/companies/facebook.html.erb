				</ul>
				<% @pages.each do |page| %>
					<div class="box">
						<%= link_to facebook_show_path(page) do %>
							<div class="profile-image">
								<%= image_tag( @graph.get_picture( page['id'], type: :large) ) %>
							</div>
							<div class="profile-name">
